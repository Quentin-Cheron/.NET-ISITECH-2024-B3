@model IEnumerable<Teacher>

<h1>
    Liste des professeures
</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Age</th>
            <th>Add</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Lastname</td>
                <td>@teacher.Firstname</td>
                <td>@teacher.Age</td>
                <td>
                    <button>Ajouter</button>
                </td>
            </tr>
        }

    </tbody>
</table>